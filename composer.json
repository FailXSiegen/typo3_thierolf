{
    "name": "riconet/t3-project-thierolf",
    "description": "t3-project-custom",
    "license": "GPL-3.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/",
            "options": {
                "ssl": {
                    "verify_peer": "false"
                }
            }
        },
        {
            "type": "path",
            "url": "extensions/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "platform": {
            "php": "8.1.24"
        },
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "helhum/dotenv-connector": true
        }
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-backend": "^12.4",
        "typo3/cms-core": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-extensionmanager": "^12.4",
        "typo3/cms-filelist": "^12.4",
        "typo3/cms-fluid": "^12.4",
        "typo3/cms-frontend": "^12.4",
        "typo3/cms-install": "^12.4",
        "typo3/cms-recordlist": "^12.4",
        "typo3/cms-about": "^12.4",
        "typo3/cms-belog": "^12.4",
        "typo3/cms-beuser": "^12.4",
        "typo3/cms-felogin": "^12.4",
        "typo3/cms-fluid-styled-content": "^12.4",
        "typo3/cms-form": "^12.4",
        "typo3/cms-impexp": "^12.4",
        "typo3/cms-info": "^12.4",
        "typo3/cms-rte-ckeditor": "^12.4",
        "typo3/cms-setup": "^12.4",
        "typo3/cms-seo": "^12.4",
        "typo3/cms-sys-note": "^12.4",
        "typo3/cms-t3editor": "^12.4",
        "typo3/cms-tstemplate": "^12.4",
        "typo3/cms-viewpage": "^12.4",
        "typo3/cms-redirects": "^12.4",
        "typo3/cms-lowlevel": "^12.4",
        "typo3/cms-recycler": "^12.4",
        "typo3/cms-dashboard": "^12.4",
        "typo3/cms-scheduler": "^12.4",
        "helhum/typo3-console": "^8",
        "helhum/dotenv-connector": "^3",
        "gridelementsteam/gridelements": "@dev",
        "laxap/bootstrap-grids": "@dev",
        "t3/min": "^3",
        "georgringer/news": "^11",
        "lochmueller/staticfilecache": "^13",
        "thierolf/ttpdf": "@dev",
        "failx/templates-thierolf": "@dev",
        "failx/templates-thierolf-subpage": "@dev",
        "plan2net/webp": "^5",
        "jweiland/sync-crop-areas": "^3.0"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    },
    "require-dev": {
        "deployer/deployer": "^7",
		"deployer/recipes": "^6",
		"rector/rector": "*",
        "ssch/typo3-rector": "^1"
    }
}
