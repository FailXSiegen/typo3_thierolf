{
    "name": "thierolf/ttpdf",
    "type": "typo3-cms-extension",
    "description": "",
    "keywords": [
      "TYPO3 CMS",
      "Extension"
    ],
    "license": [
      "GPL-3.0-or-later"
    ],
    "require": {
      "typo3/cms-core": ">=10.4"
    },
    "replace": {
      "ttpdf": "self.version"
    },
    "autoload": {
      "psr-4": {
        "Thierolf\\Ttpdf\\": "Classes/"
      }
    },
    "config": {
      "vendor-dir": ".Build/vendor",
      "bin-dir": ".Build/bin",
      "platform": {
        "php": "7.4"
      }
    },
    "extra": {
      "typo3/cms": {
        "extension-key": "ttpdf",
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/Web"
      }
    }
  }
  