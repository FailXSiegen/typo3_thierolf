{
    "name": "nbg/new-bootstrap-gridelements",
    "type": "typo3-cms-extension",
    "description": "",
    "keywords": [
      "TYPO3 CMS",
      "Extension"
    ],
    "license": [
      "GPL-3.0-or-later"
    ],
    "require": {
      "typo3/cms-core": ">=10.4",
      "gridelementsteam/gridelements": ">=8.4"
    },
    "replace": {
      "new_bootstrap_elements": "self.version",
      "typo3-ter/fx-templates": "self.version"
    },
    "config": {
      "vendor-dir": ".Build/vendor",
      "bin-dir": ".Build/bin",
      "platform": {
        "php": "7.4"
      }
    },
    "extra": {
      "typo3/cms": {
        "extension-key": "new_bootstrap_gridelements",
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/Web"
      }
    }
  }
  